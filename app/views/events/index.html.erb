<%= month_calendar events: @events do |date, events| %>
  <%= date.day %>
  
  <ul class="event-list">
    <% events.each do |event| %>
    <li class="list">
      <%= link_to edit_event_path(event.id) do %>
        <%= event.plan.call %>
        <%= event.name %>
        <%= event.num.name %>
      <% end %>
    </li>
  </ul>
      
  <% end %>
<% end %>


